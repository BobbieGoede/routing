{
  "name": "vue-i18n-routing",
  "description": "The i18n routing with using vue-i18n",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "tsc -p . --noEmit",
    "build:docs": "api-docs-gen ./temp/vue-i18n-routing.api.json -c ./docsgen.config.js -o ./ -g noprefix"
  },
  "dependencies": {
    "@intlify/shared": "beta",
    "@intlify/vue-router-composable": "latest",
    "vue-demi": "*"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.18.20",
    "api-docs-gen": "^0.4.0",
    "rimraf": "^3.0.2",
    "vite": "^2.7.4",
    "vite-plugin-dts": "^0.9.6",
    "vue": "^3.2.23",
    "vue-i18n": "npm:vue-i18n@next",
    "vue-i18n-bridge": "beta",
    "vue-i18n-legacy": "npm:vue-i18n@latest",
    "vue-router": "^4.0.12",
    "vue-router3": "npm:vue-router@latest",
    "vue2": "npm:vue@2"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.4.0",
    "vue": "^2.6.14 || ^3.2.0",
    "vue-router": "^3.5.3 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "keywords": [
    "i18n",
    "internationalization",
    "intlify",
    "routing"
  ],
  "files": [
    "dist",
    "index.mjs",
    "LICENSE",
    "README.md"
  ],
  "main": "./dist/vue-i18n-routing.cjs.js",
  "module": "./dist/vue-i18n-routing.es.js",
  "unpkg": "dist/vue-i18n-routing.iife.js",
  "jsdelivr": "dist/vue-i18n-routing.iife.js",
  "types": "dist/vue-i18n-routing.d.ts",
  "exports": {
    ".": {
      "import": {
        "node": "./index.mjs",
        "default": "./dist/vue-i18n-routing.es.js"
      },
      "require": "./dist/vue-i18n-routing.cjs.js"
    }
  },
  "license": "MIT",
  "author": {
    "name": "kazuya kawaguchi",
    "email": "kawakazu80@gmail.com"
  },
  "homepage": "https://github.com/intlify/routing/tree/main/packages/vue-i18n-routing#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intlify/routing.git",
    "directory": "packages/vue-i18n-routing"
  },
  "bugs": {
    "url": "https://github.com/intlify/routing/issues"
  },
  "engines": {
    "node": ">= 12"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
